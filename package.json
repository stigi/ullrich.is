{
  "name": "gatsby-starter-hello-world",
  "description": "Gatsby hello world starter",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "graphql-types": "gql-gen --url http://localhost:8000/___graphql --template typescript --out ./src/graphql-types.d.ts",
    "deploy:gh-pages": "gh-pages -r git@github.com:stigi/stigi.github.io.git -d public -b master",
    "deploy": "npm-run-all build deploy:gh-pages",
    "lint": "npm-run-all -c lint:prettier lint:tslint",
    "lint:prettier": "prettier -c '**/*.{js,jsx,ts,tsx,json,md,yml}'",
    "lint:tslint": "tslint  -t verbose --project .",
    "format": "npm-run-all format:prettier format:tslint",
    "format:prettier": "yarn lint:prettier --write",
    "format:tslint": "yarn lint:tslint --fix",
    "graphql-update-types": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "babel-plugin-styled-components": "^1.8.0",
    "gatsby": "^2.0.0",
    "gatsby-image": "^2.0.15",
    "gatsby-plugin-favicon": "^3.1.4",
    "gatsby-plugin-google-analytics": "^2.0.6",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.7",
    "gatsby-plugin-styled-components": "^3.0.0",
    "gatsby-plugin-typescript": "^2.0.0",
    "gatsby-source-filesystem": "^2.0.5",
    "gatsby-transformer-remark": "^2.1.10",
    "gatsby-transformer-sharp": "^2.1.4",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.0.2",
    "typescript": "^3.1.3"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^1.0.7",
    "@graphql-codegen/cli": "^1.0.7",
    "@graphql-codegen/typescript": "1.0.7",
    "@graphql-codegen/typescript-operations": "1.0.7",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.0.1",
    "gh-pages": "^2.0.1",
    "graphql": "^0.13.2",
    "husky": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0"
  }
}
